<!-- Web.Config Configuration File -->
<configuration>
	<!--
		For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

		The following attributes can be set on the <httpRuntime> tag.
			<system.Web>
				<httpRuntime targetFramework="4.5" />
			</system.Web>
	-->
	<system.web>
		<customErrors mode="Off"/>
		<compilation debug="true" targetFramework="4.5">
		</compilation>
		<authentication mode="Windows"/>
		<httpRuntime/>
		<pages validateRequest="false" controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID">
		</pages>
	</system.web>
	<appSettings>
		<add key="bidon" value="rien"/>
		<add key="DefaultPassword" value="croix"/>
		<add key="DATABASE_NAME" value="CCC_SONGS"/>
		<add key="DB_SERVER" value=".\SQLEXPRESS"/>
		<add key="DB_USER" value="CCC_website"/>
		<add key="DB_PASSWORD" value="croix"/>
	</appSettings>
	<connectionStrings>
		<add name="strCnx" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=CCC_SONGS;User ID=CCC_website;pwd=croix;" providerName="System.Data.SqlClient"/>
	</connectionStrings>
</configuration>
